{
  "name": "@blackbaud/stache",
  "version": "2.0.0-beta.6",
  "description": "Angular 2 components for SKY UX Builder SPAs",
  "author": "Blackbaud, Inc.",
  "keywords": [],
  "homepage": "https://github.com/blackbaud/stache2",
  "repository": {
    "type": "git",
    "url": "https://github.com/blackbaud/stache2.git"
  },
  "bugs": {
    "url": "https://github.com/blackbaud/stache2/issues"
  },
  "license": "MIT",
  "main": "bundles/stache.umd.js",
  "module": "index.ts",
  "engines": {
    "node": ">=6.9.1"
  },
  "scripts": {
    "skyux": "skyux",
    "clean": "rimraf dist .srctmp && npm run clean:ngc",
    "clean:ngc": "rimraf **/*.ngsummary.json **/*.ngfactory.ts **/*.ngstyle.ts",
    "prepare:ts": "npm run clean && node ./scripts/stage-ts.js",
    "prepare:package": "node ./scripts/prepare-package.js",
    "transpile": "webpack --config ./config/webpack/webpack.release.config.js --progress",
    "build": "npm run clean && npm run prepare:ts && npm run transpile && npm run minify && npm run prepare:package && npm run clean:ngc",
    "minify": "uglifyjs dist/bundles/stache.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/stache.umd.min.js",
    "test": "skyux test"
  },
  "peerDependencies": {
    "codelyzer": "~3.0.1",
    "fs-extra": "~3.0.1",
    "glob": "~7.1.2",
    "node-sass": "~4.5.3",
    "webpack": "~2.6.1",
    "typescript": "~2.3.4"
  },
  "dependencies": {
    "@blackbaud/skyux": "2.0.0-rc.0",
    "@blackbaud/skyux-builder-plugin-stache": "~1.0.0",
    "prismjs": "1.6.0",
    "smoothscroll-polyfill": "0.3.5"
  },
  "devDependencies": {
    "@blackbaud/skyux-builder": "1.0.0-rc.2",
    "ngc-webpack": "3.0.0",
    "rimraf": "2.6.1",
    "uglify-js": "3.0.18"
  }
}
