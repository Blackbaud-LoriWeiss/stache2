{
  "name": "stache2",
  "mode": "easy",
  "compileMode": "aot",
  "auth": false,
  "plugins": [
    "@blackbaud/skyux-builder-plugin-stache"
  ],
  "app": {
    "title": "Stache 2"
  },
  "omnibar": {
    "experimental": true,
    "nav": {
      "services": [
        {
          "title": "Stache 2",
          "items": [
            {
              "title": "Home",
              "route": "/"
            },
            {
              "title": "Demos",
              "route": "/demos"
            }
          ]
        }
      ]
    }
  },
  "appSettings": {
    "stache": {
      "editButton": {
        "url": "https://github.com/blackbaud/stache2",
        "text": "Suggest Edits"
      },
      "searchConfig": {
      },
      "footer": {
        "nav": [
          {
            "title": "Privacy Policy",
            "route": "/demos/privacy-policy"
          },
          {
            "title": "Terms of Use",
            "route": "/demos/anchor-link"
          }
        ]
      },
      "googleAnalytics": {
        "enabled": "true",
        "clientId": "UA-2418840-1",
        "tagManagerContainerId": "GTM-W56QP9"
      }
    }
  }
}
